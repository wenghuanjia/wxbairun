.img_cover {
  margin-bottom: 20rpx;
  box-sizing: border-box;
  padding-left: 30rpx;
  width: 100%;
  height: 173rpx;
  background-image: url(http://qiniu.gzyanlian.com/微信截图_20200910105137_meitu_1.png);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-evenly;
  color: #fff;
}

.service_title {
  font-size: 16px;
  font-weight: 700;
}

.service_desc {
  font-size: 13px;
}

.service_desc .tip {
  font-size: 16px;
  font-weight: 700;
}

.computed {
